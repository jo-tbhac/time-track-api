generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/db.sqlite3"
}

model Job {
  id         Int    @id @default(autoincrement())
  name       String
  hourlyWage Int    @map("hourly_wage")

  @@map("jobs")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique

  @@map("users")
}
